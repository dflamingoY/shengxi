<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorContentBackground"
    android:clipChildren="false"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/frame_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/actionbarView">

    </FrameLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_above="@+id/actionbarView"
        android:background="@mipmap/icon_action_up" />

    <org.xiaoxingqi.shengxi.wedgit.actionTabar.ActionBarView
        android:id="@+id/actionbarView"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal" />

    <org.xiaoxingqi.shengxi.wedgit.ThemeSetView
        android:id="@+id/themeSetView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</RelativeLayout>


